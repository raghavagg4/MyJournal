{
    "version": 2,
    "builds": [
        {
            "src": "Journal/run.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "Journal/flask_app/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "Journal/run.py"
        }
    ],
    "env": {
        "PYTHONPATH": "."
    }
}
